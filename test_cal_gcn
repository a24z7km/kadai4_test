#!/bin/bash

# 引数のチェック
if [ $# -ne 2 ]; then
    echo "エラー: 2つの自然数を引数として指定してください。" >&2
    exit 1
fi

# 引数が自然数かどうかを確認
if ! [[ $1 =~ ^[1-9][0-9]*$ ]] || ! [[ $2 =~ ^[1-9][0-9]*$ ]]; then
    echo "エラー: 引数は自然数である必要があります。" >&2
    exit 1
fi

# 最大公約数を計算する関数
gcd() {
    local a=$1
    local b=$2
    while [ $b -ne 0 ]; do
        local temp=$b
        b=$((a % b))
        a=$temp
    done
    echo $a
}

# 最大公約数を計算して出力
result=$(gcd $1 $2)
echo $result
